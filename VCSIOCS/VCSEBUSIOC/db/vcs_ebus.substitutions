####################### Load relay modules Template for SIS interface ###################################################################
##### Interface to Safety Interlock System ##########
# The SIS interface is done via 5 relay output Module(EL2624).  
# These output are availlable on output card 4 connector 8
file "db/EL2624.template" {
pattern {
 SYS  	DEVICE  DEVSCAN     SCAN 		PORT	OUTPUT1	    OUTPUT2	    OUTPUT3	    OUTPUT4   }
{VCS  	ROM1	"I/O Intr"  "Passive"   ERIO.1  SPC1-CLR    SPC2-CLR	SPC2L-CLR	cnA-CLR    }
{VCS  	ROM2	"I/O Intr"  "Passive"   ERIO.2  BSV-CLR     TRL-CLR		VLI-CLR  	HLI-CLR    }
{VCS  	ROM3	"I/O Intr"  "Passive"   ERIO.3  SPE-CLR     EV3-CLR		EV2-CLR    	EV1-CLR    }
{VCS  	ROM4	"I/O Intr"  "Passive"   ERIO.4  EXP-CLR     IBL-CLR		SSC-CLR  	SSCL-CLR   }
{VCS  	ROM5	"I/O Intr"  "Passive"   ERIO.5  SSCCAB-CLR	ROM5:ONC-1	ROM5:ONC-2 	ROM5:ONC-3 }
}